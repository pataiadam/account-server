<section class="content">
  <div class="row">
    <div class="col-lg-3 col-xs-6">
        <!-- small box -->
        <div class="small-box bg-yellow">
            <div class="inner">
                <h3>{{this.usersCount}}</h3>
                <p>Users count</p>
            </div>
            <div class="icon">
                <i class="ion ion-person-add"></i>
            </div>
            <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
        </div>
    </div>
    <div class="col-lg-3 col-xs-6">
        <div class="small-box bg-green">
            <div class="inner">
                <h3>{{this.passportsCount}}</h3>
                <p>Passports count</p>
            </div>
            <div class="icon">
                <i class="ion ion-stats-bars"></i>
            </div>
            <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
        </div>
    </div>

      <div class="col-xs-12 col-lg-6">
          <div class="box">
              <div class="box-header">
                  <h3 class="box-title">Most used providers (number of API calls)</h3>
              </div>
              <!-- /.box-header -->
              <div class="box-body no-padding">
                  <table class="table table-striped">
                      <tbody><tr>
                          <th>Provider</th>
                          <th>Last hour</th>
                          <th>Last day</th>
                          <th>Last week</th>
                      </tr>
                      {{#each this.providerStats}}
                          <tr>
                              <td>{{@key}}</td>
                              <td>{{this.hour}}</td>
                              <td>{{this.day}}</td>
                              <td>{{this.week}}</td>
                          </tr>
                      {{/each}}
                      </tbody></table>
              </div>
              <!-- /.box-body -->
          </div>
          <!-- /.box -->
      </div>
  </div>
</section>